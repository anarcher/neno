{
  "name": "neno",
  "version": "1.0.0",
  "description": "NENO is an app that helps you creating your personal knowledge graph.",
  "type": "module",
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@editorjs/editorjs": "2.22.2",
    "@editorjs/header": "^2.6.2",
    "@editorjs/list": "^1.6.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@tippyjs/react": "^4.2.6",
    "@types/express": "^4.17.13",
    "archiver": "^5.0.2",
    "babel-loader": "^8.2.3",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.10",
    "commander": "^8.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "d3": "^7.2.1",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "express-session": "^1.17.2",
    "idb-keyval": "^6.0.3",
    "node-2fa": "^2.0.3",
    "node-fetch": "^3.1.0",
    "qrcode-terminal": "^0.12.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-router-dom": "^6.1.1",
    "tippy.js": "^6.3.7",
    "ts-loader": "^9.2.5",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^6.4.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "style-loader": "^3.3.1",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "compile-typescript": "npx tsc",
    "start-server-dev": "node dist/backend/index.js --port 8080 --session-secret 000",
    "start-server-dev-tls": "node dist/backend/index.js --https-port 8080 --use-https --cert-path ~/.neno/server.cert --cert-key-path ~/.neno/server.key",
    "build-frontend-production": "npx webpack --config frontend/webpack.production.js",
    "build-frontend-production-local-only": "npx webpack --config frontend/webpack.production.local-only.js",
    "build-frontend-development": "npx webpack --config frontend/webpack.development.js",
    "build-frontend-development-local-only": "npx webpack --config frontend/webpack.development.local-only.js",
    "build": "npm run compile-typescript && npm run build-frontend-production",
    "start-dev-server-frontend": "npx webpack serve --config frontend/webpack.development.js",
    "start-dev-server-frontend-local-only": "npx webpack serve --config frontend/webpack.development.local-only.js",
    "start": "npm run build && npm run start-server-dev",
    "start-tls": "npm run build && npm run start-server-dev-tls",
    "dev": "concurrently -i --default-input-target 0 --kill-others \"npm run compile-typescript && npm run start-server-dev\" \"npm run build-frontend-development\"",
    "dev-local-only": "concurrently -i --default-input-target 0 --kill-others \"npm run compile-typescript && npm run start-server-dev\" \"npm run build-frontend-development-local-only\""
  },
  "author": "Sebastian Zimmer",
  "license": "Apache License 2.0"
}
